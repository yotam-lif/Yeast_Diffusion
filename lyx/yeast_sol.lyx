\begin_layout Standard
Now solve:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\nabla^{2}C(x,z) & =0
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\frac{1}{X(x)}\partial_{xx}X(x) & =-k^{2}=-\frac{1}{Z}\partial_{zz}Z(z)
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
X_{n}(x) & =A_{n}e^{+ik_{n}x}+B_{n}e^{-ik_{n}x}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
Z_{n}(z) & =C_{n}e^{+k_{n}z}+D_{n}e^{-k_{n}z}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
BC 3:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
C(x,z)|_{x=0} & \propto A_{n}+B_{n}+\frac{r}{2\Omega}=\frac{r}{2\Omega}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\Rightarrow B_{n}=-A_{n}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
 & \Rightarrow X_{n}(x)=A_{n}sin(k_{n}x)
\end{align}

\end_inset


\end_layout

\begin_layout Standard
BC 4:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\partial_{x}C(x,z)|_{x=-L_{x}} & \propto cos(k_{n}L_{x})=0
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
k_{n} & =\frac{\pi}{2L_{x}}(2n+1)\Rightarrow k_{n_{odd}}=\frac{\pi}{2L_{x}}n_{odd}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Where we have only odd wavenumbers.
\end_layout

\begin_layout Standard
BC 1:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\partial_{z}C(x,z)|_{z=L_{z}} & \propto C_{n}e^{+k_{n}L_{z}}-D_{n}e^{-k_{n}L_{z}}=0
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
D_{n} & =C_{n}e^{+2k_{n}L_{z}}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
 & \Rightarrow Z_{n}(z)\propto(e^{+k_{n}z}+e^{+2k_{n}L_{z}}e^{-k_{n}z})\propto cosh(k_{n}(z-L_{z}))
\end{align}

\end_inset


\end_layout

\begin_layout Standard
So we now have for 
\begin_inset Formula $x<0$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
C(x,z)=\sum_{n_{odd}}A_{n}sin(k_{n}x)cosh(k_{n}(z-L_{z}))
\]

\end_inset


\end_layout

\begin_layout Standard
And our final BC is (on 
\begin_inset Formula $x<0$
\end_inset

):
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\partial_{z}C(x,z)|_{z=0} & =-r+\Omega C(x,z=0)
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\sum-k_{n}A_{n}cos(k_{n}x)sinh(k_{n}L_{z}) & =+\tilde{\Omega}\sum A_{n}sin(k_{n}x)cosh(k_{n}L_{z})-\frac{r}{2}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\sum_{n_{odd}}A_{n}sin(k_{n}x)[\Omega cosh(k_{n}L_{z})+k_{n}sinh(k_{n}L_{z})] & =\frac{r}{2}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
A_{n}^{'} & :=A_{n}\frac{\Omega cosh(k_{n}L_{z})+k_{n}sinh(k_{n}L_{z})}{r/2}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\sum_{n_{odd}}A_{n}^{'}sin(k_{n}x) & =1
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Multiply by 
\begin_inset Formula $sin(k_{m}x)$
\end_inset

 and integrate on 
\begin_inset Formula $x<0$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
A_{m}^{'}\frac{L_{x}}{2} & =\int_{-L_{x}}^{0}sin(k_{m}x)dx=-\frac{1}{k_{m}}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Where we get that for 
\begin_inset Formula $m_{odd}\neq n_{odd}$
\end_inset

 these sines are orthogonal on 
\begin_inset Formula $[-L_{x},0].$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
-\frac{2}{k_{n}L_{x}}=A_{n}^{'}=A_{n}\frac{\Omega cosh(k_{n}L_{z})+k_{n}sinh(k_{n}L_{z})}{r/2}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\Rightarrow A_{n}=-\frac{r}{L_{x}}\frac{1}{k_{n}(\Omega cosh(k_{n}L_{z})+k_{n}sinh(k_{n}L_{z}))}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
C_{<}(x,z)=\frac{r}{2\Omega}-\frac{r}{L_{x}}\sum_{n_{odd}}\frac{sin(k_{n}x)cosh(k_{n}(z-L_{z}))}{k_{n}(\Omega cosh(k_{n}L_{z})+k_{n}sinh(k_{n}L_{z}))}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
And this actually agrees with the symmetry we found, so this is the solution
 on all 
\begin_inset Formula $x$
\end_inset

: 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
C(x,z)=\frac{r}{2\Omega}-\frac{r}{L_{x}}\sum_{n_{odd}}\frac{sin(k_{n}x)cosh(k_{n}(z-L_{z}))}{k_{n}(\Omega cosh(k_{n}L_{z})+k_{n}sinh(k_{n}L_{z}))}
\end{equation}

\end_inset


\end_layout

